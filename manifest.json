{
  "manifest_version": 2,
  "name": "Unget Pocket",
  "version": "1.0.1",
  "description": "Redirect getpocket.com articles to their origin",
  "author": "Daniel Rozenberg",
  "homepage_url": "https://github.com/danielrozenberg/unget-pocket",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://getpocket.com/explore/item/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
